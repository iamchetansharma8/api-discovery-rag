{
    "swagger": "2.0",
    "info": {
        "version": "1.0",
        "title": "Upload Document API",
        "description": "The Document Management API provides programmatic access to our document repository, enabling applications to manage documents with increased efficiency, improved information control, and reduced cost."
    },
    "basePath": "/documents/v1.0",
    "paths": {
        "/upload": {
            "post": {
                "tags": [
                    "Document Management"
                ],
                "summary": "Store a new document",
                "description": "Store a new document in the repository. A JSON map containing all agreed metadata must be provided. Supported metadata items are agreed as part of the onboarding process.",
                "operationId": "uploadUsingPOST",
                "consumes": [
                    "multipart/form-data"
                ],
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "Authorization",
                        "in": "header",
                        "description": "JWT Bearer Token",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "in": "formData",
                        "name": "MetaData",
                        "description": "A JSON map containing key/value pairs for all agreed metadata items.",
                        "required": true,
                        "type": "string",
                        "default": "{ \"ArrangementID\": \"IL01003006\", \"BankingCustomerIdentifier\": \"9999888938\", \"BusinessProcess\": \"tyl\", \"Description\": \"Sample.doc\", \"DocumentReferenceID\": \"DATA7 GMT\", \"DocumentSource\": \"tyl\", \"DocumentTitle\": \"test doc\", \"filename\": \"Sample.doc\", \"IsCustomerVisibleDocument\": \"true\", \"DeletedFlag\": \"false\", \"Language\": \"ENG\", \"mimeType\": \"application/msword\", \"Name\": \"CDT-27888.doc\", \"PartyID\": \"9999888938\", \"RBSClassification\": \"Confidential\", \"RBSCompany\": \"NWG\", \"RBSHighRiskRecord\": \"High Risk\", \"RBSJurisdiction\": \"GB\", \"RBSPersonalInfo\": \"true\", \"RecordClassCode\": \"RM1100\", \"RecordType\": \"targeted_due_dil\", \"AccountNumber\": \"123\", \"CustomerReferenceID\": \"123\", \"LegalEntityID\": \"123\", \"LastUsedID\": \"y\", \"SortCode\": \"123\", \"ReasonForDeletion\": \"y\", \"rmCreationDate\": \"2021-04-14T12:39:43.221\" }"
                    },
                    {
                        "name": "file",
                        "in": "formData",
                        "description": "The file to be uploaded to the repository.",
                        "required": true,
                        "type": "file"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "$ref": "#/definitions/UploadResponseObject"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                },
                "deprecated": false
            }
        }
    },
    "definitions": {
        "UploadResponseObject": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "format": "int32",
                    "example": 201,
                    "description": "HTTP style response code"
                },
                "message": {
                    "type": "string",
                    "description": "Status message from the API",
                    "example": "Success response"
                },
                "objectId": {
                    "type": "string",
                    "example": "e86411ad-579d-48b8-b4b8-9f14b1e8bcdd",
                    "description": "ID of the object created within the repository"
                },
                "status": {
                    "type": "string",
                    "example": "CREATED",
                    "description": "HTTP style response message"
                }
            },
            "title": "UploadResponseObject"
        }
    }
}